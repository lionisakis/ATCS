source activate llama_3_instruct

python modules/main_2.py --model_name model_zoo/meta-llama/suzume-llama-3-8B-multilingual --target_group female --template 3
python modules/main_2.py --model_name model_zoo/meta-llama/suzume-llama-3-8B-multilingual --target_group male --template 3